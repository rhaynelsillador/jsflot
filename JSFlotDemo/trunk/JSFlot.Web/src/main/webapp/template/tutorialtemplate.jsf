        iconExpandedGroup="disc" iconCollapsedGroup="disc" 
        iconExpandedTopGroup="chevronUp" iconGroupTopPosition="right" 
        iconCollapsedTopGroup="chevronDown">
			<rich:panelMenuGroup label="jsflot:resources" expanded="true">
				<rich:panelMenuItem label="jsflot:resources">
				
				</rich:panelMenuItem>
			</rich:panelMenuGroup>
			<rich:panelMenuGroup label="jsflot:flotChart" expanded="true">
				<rich:panelMenuItem label="actionListener">
				
				</rich:panelMenuItem>
				<rich:panelMenuItem label="reRender">
				
				</rich:panelMenuItem>
			</rich:panelMenuGroup>
		</rich:panelMenu>
	</ui:insert>
	
	<rich:panel style="width: 100%;">
		<ui:insert name="tutorialContents">
			TutContents
		</ui:insert>
	</rich:panel>
	
	</h:panelGrid>
	
	
</html>