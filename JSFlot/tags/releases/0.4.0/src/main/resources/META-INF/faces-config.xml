<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
  version="1.2">
<component>
	<description>
		Renderers a Chart based on the Flot JavaScript Charting Library
	</description>
	<component-type>org.jsflot.components.FlotChartComponent</component-type>
	<component-class>org.jsflot.components.FlotChartComponent</component-class>
	<component-extension>
      <component-family>org.jsflot.components.FlotChartComponent</component-family>
      <renderer-type>org.jsflot.components.FlotChartRenderer</renderer-type>
    </component-extension>
</component>

<render-kit>
	<renderer>
		<component-family>org.jsflot.components.FlotChartComponent</component-family>
		<renderer-type>org.jsflot.components.FlotChartRenderer</renderer-type>
		<renderer-class>org.jsflot.components.FlotChartRenderer</renderer-class>
	</renderer>
</render-kit>

<component>
	<description>
		Renderers a Chart based on the Flot JavaScript Charting Library
	</description>
	<component-type>org.jsflot.components.ResourcesComponent</component-type>
	<component-class>org.jsflot.components.ResourcesComponent</component-class>
	<component-extension>
      <component-family>org.jsflot.components.ResourcesComponent</component-family>
      <renderer-type>org.jsflot.components.ResourcesRenderer</renderer-type>
    </component-extension>
</component>

<render-kit>
	<renderer>
		<component-family>org.jsflot.components.ResourcesComponent</component-family>
		<renderer-type>org.jsflot.components.ResourcesRenderer</renderer-type>
		<renderer-class>org.jsflot.components.ResourcesRenderer</renderer-class>
	</renderer>
</render-kit>

<lifecycle>
	<phase-listener>org.jsflot.renderkit.phaselistener.ResourceLoaderPhaseListener</phase-listener>
</lifecycle>
</faces-config>